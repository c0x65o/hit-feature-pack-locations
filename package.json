{
  "name": "@hit/feature-pack-locations",
  "version": "1.0.26",
  "description": "Locations feature pack with hierarchical locations, user assignments, map visualization, and geocoding. Uses @hit/ui-kit for shell-agnostic UI.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./pages/*": {
      "types": "./dist/pages/*.d.ts",
      "import": "./dist/pages/*.js",
      "require": "./dist/pages/*.js",
      "default": "./dist/pages/*.js"
    },
    "./schema": {
      "types": "./dist/schema/locations.d.ts",
      "import": "./dist/schema/locations.js",
      "require": "./dist/schema/locations.js",
      "default": "./dist/schema/locations.js"
    },
    "./server/api/*": {
      "types": "./dist/server/api/*.d.ts",
      "import": "./dist/server/api/*.js",
      "require": "./dist/server/api/*.js",
      "default": "./dist/server/api/*.js"
    }
  },
  "files": [
    "dist",
    "feature-pack.yaml",
    "migrations"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "@hit/sdk": "*",
    "@hit/ui-kit": "*",
    "drizzle-orm": ">=0.30.0",
    "leaflet": "^1.9.0",
    "lucide-react": ">=0.263.0",
    "next": "^14.0.0",
    "react": "^18.0.0",
    "react-leaflet": "^4.0.0 || ^5.0.0"
  },
  "devDependencies": {
    "@hit/ui-kit": "file:../hit-ui-kit",
    "@types/leaflet": "^1.9.21",
    "@types/node": "^20.19.26",
    "@types/react": "^18.2.0",
    "drizzle-orm": "^0.30.0",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.460.0",
    "next": "^14.2.5",
    "react-leaflet": "^4.2.1",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "hit",
    "feature-pack",
    "locations",
    "maps",
    "geocoding",
    "crud",
    "ui-kit"
  ],
  "license": "MIT"
}
